{
  "cli": {
    "version": ">= 0.52.0"
  },
  "build": {
    "fast": {
      "ios": {
        "resourceClass": "m1-medium"
      }
    },
    "development": {
      "extends": "fast",
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "extends": "fast",
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "extends": "fast",
      "channel": "production"
    },
    "simulator": {
      "extends": "development",
      "ios": {
        "simulator": true
      }
    }
  },
  "submit": {
    "production": {}
  }
}
