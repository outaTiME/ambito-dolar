{
  "cli": {
    "version": ">= 4.1.2"
  },
  "build": {
    "fast": {
      "ios": {
        "resourceClass": "m-medium",
        "image": "latest"
      },
      "channel": "fast"
    },
    "development": {
      "extends": "fast",
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "extends": "fast",
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "extends": "fast",
      "channel": "production"
    },
    "simulator": {
      "extends": "development",
      "ios": {
        "simulator": true
      },
      "channel": "simulator"
    }
  },
  "submit": {
    "production": {}
  }
}
