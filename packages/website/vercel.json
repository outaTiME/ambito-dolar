{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://api.ambito-dolar.app/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "status": 404,
      "dest": "/404.html"
    }
  ]
}
