{
  "name": "ambito-dolar",
  "license": "CC-BY-NC-ND-4.0",
  "repository": "outaTiME/ambito-dolar",
  "author": {
    "name": "outaTiME",
    "email": "afalduto@gmail.com",
    "url": "https://outa.im"
  },
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@serverless-stack/cli": "1.2.26",
    "@serverless-stack/resources": "1.2.26",
    "aws-cdk-lib": "2.24.0",
  },
  "scripts": {
    "test": "lerna run test",
    "g:eslint": "cd $INIT_CWD && eslint .",
    "client:start": "yarn workspace @ambito-dolar/client run start",
    "client:build:ios": "yarn workspace @ambito-dolar/client expo build:ios",
    "client:build:android": "yarn workspace @ambito-dolar/client expo build:android",
    "client:build:web": "yarn workspace @ambito-dolar/client expo build:web",
    "client:publish": "yarn workspace @ambito-dolar/client expo publish",
    "client:publish:web": "vercel packages/client/web-build",
    "client:upgrade": "yarn workspace @ambito-dolar/client expo upgrade",
    "api:start": "yarn workspace @ambito-dolar/api run start",
    "api:start": "yarn sst start",
    "api:publish": "yarn sst deploy",
    "api:remove": "yarn sst remove --debug-stack",
    "publish": "lerna publish",
    "version": "yarn install && git stage yarn.lock",
    "depcheck": "yarn workspaces foreach --verbose exec npx depcheck"
  },
  "resolutions": {
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "sharp": "^0.28.0"
  },
  "packageManager": "yarn@3.2.0"
}
